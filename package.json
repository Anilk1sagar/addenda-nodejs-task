{
  "name": "nodejs-test-addenda",
  "version": "1.0.0",
  "description": "Nodejs Test Addenda",
  "main": "index.js",
  "scripts": {
    "start": "SET NODE_ENV=local && cross-env NODE_ENV=local && nodemon src/server.js --exec babel-node",
    "start-dev": "cross-env NODE_ENV=development && nodemon src/server.js --exec babel-node",
    "start-prod": "SET NODE_ENV=production && cross-env NODE_ENV=production && nodemon src/server.js --exec babel-node",
    "debug:dev": "npm run build && cross-env NODE_ENV=local && nodemon dist/server.js",
    "build": "babel src -d dist",
    "server:dev": "npm run build && pm2-runtime start ecosystem.config.json --env development",
    "server:prod": "npm run build && pm2-runtime start ecosystem.config.json --env production",
    "stop:server": "pm2 stop ecosystem.config.json",
    "lint": "./node_modules/.bin/eslint src/base_backend.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Anilk1sagar/base_backend.git"
  },
  "author": "Anil Sagar",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/Anilk1sagar/base_backend/issues"
  },
  "homepage": "https://gitlab.com/Anilk1sagar/base_backend#README",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.9"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^3.1.0",
    "dotenv": "^8.0.0",
    "dotenv-safe": "^6.1.0",
    "express": "^4.17.1",
    "express-boom": "^3.0.0",
    "express-jwt": "^5.3.1",
    "express-jwt-permissions": "^1.3.1",
    "express-rate-limit": "^5.0.0",
    "express-validator": "^5.3.0",
    "express-winston": "^3.2.1",
    "helmet": "^3.20.0",
    "js-sha512": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.6.7",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.1",
    "sequelize": "^4.38.0",
    "sequelize-fixtures": "^0.9.0",
    "util": "^0.11.0",
    "uuid": "^3.3.2",
    "winston": "^3.0.0",
    "winston-daily-rotate-file": "^3.3.0"
  }
}
